{
  "name": "vuu-ui",
  "version": "0.0.2",
  "author": "heswell <steve@heswell.com>",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "packages/**",
    "sample-apps/**",
    "showcase",
    "salt/packages/lab"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.jsx",
    "prettier": "prettier './**/*.{js,,mjs,jsx,css,md,json}' --config ./.prettierrc",
    "format": "prettier --write './**/*.{js,,mjs,jsx,css,md,json}' --config ./.prettierrc",
    "build": "node ./scripts/build-all.mjs",
    "build:app": "cd sample-apps/app-vuu-example && node scripts/build.mjs",
    "build:packages": "yarn build --cjs && yarn type-defs",
    "build:packages:debug": "yarn build --cjs --debug && yarn type-defs --debug",
    "build:salt": "node ./scripts/build-salt.mjs",
    "launch:demo": "node ./scripts/launch-app.mjs",
    "deploy:websocket-test": "node ./tools/deploy-websocket-test.mjs",
    "view-bundle": "node ./scripts/visualize-bundle.mjs",
    "type-defs": "node ./scripts/build-all-type-defs.mjs",
    "start:electron": "cd packages/electron && node ./node_modules/.bin/electron .",
    "showcase": "cd showcase && yarn showcase",
    "showcase:build": "cd packages/vuu-data && yarn build && cd ../../showcase && yarn copy-preview && vite build",
    "showcase:preview": "cd showcase && yarn preview",
    "test:vite": "vitest run",
    "bump": "node ./scripts/version.mjs",
    "pub": "node ./scripts/publish.mjs",
    "pub:debug": "node ./scripts/publish.mjs --debug",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/parser": "^5.41.0",
    "classnames": "^2.3.1",
    "clsx": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recoil": "^0.1.3"
  },
  "devDependencies": {
    "@heswell/component-anatomy": "0.0.5",
    "@lezer/common": "^1.0.2",
    "@lezer/generator": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "chalk": "^5.2.0",
    "esbuild": "0.17.11",
    "esbuild-visualizer": "^0.4.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "open": "^8.4.0",
    "prettier": "2.8.4",
    "tinycolor2": "1.4.2",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.29.2"
  },
  "resolutions": {
    "minimatch": "3.1.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
