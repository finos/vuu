  },
  "files": [
    "index.js",
    "/src"
  ],
  "dependencies": {
    "@vuu-ui/utils": "0.0.21",
    "antlr4-c3": "^1.1.12",
    "antlr4ts": "^0.5.0-alpha.3",
    "assert": "2.0.0"
  },
  "peerDependencies": {
    "@vuu-ui/utils": "0.0.21"
  },
  "devDependencies": {
    "antlr4ts-cli": "^0.5.0-alpha.4"
  }
}
